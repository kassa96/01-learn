{
  "compilerOptions": {
    "target": "es5",
    "module": "es6",
    "lib": ["es2015", "dom"],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,                           /* Specify what module code is generated. */
    "outDir": "./static/js",                                   /* Specify an output folder for all emitted files. */
                                    /* Skip type checking all .d.ts files. */
  },
  "include": [
    "dev/ts/**/*.ts"  
  ], 
}
